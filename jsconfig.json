{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "#/*": ["*"],
        "#pkg": ["./package.json"],
        "#bot": ["src/bot.js"],
        "#assets/*": ["./src/assets/*"],
        "#cmd/*": ["./src/commands/*"],
        "#events/*": ["./src/events/*"],
        "#models/*": ["./src/models/*"],
        "#modules/*": ["./src/modules/*"],
        "#utils/*": ["./src/utils/*"]
      },
      "moduleResolution": "node",
      "module": "ESNext",
      "target": "ESNext",
      "checkJs": true
    },
    "include": [
      "src/**/*",
      "types/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  